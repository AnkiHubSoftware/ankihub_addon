# These are used by VSCode prior to starting Anki.
export ANKI_EXEC=<CHANGE ME>
export GOOGLE_API_KEY=<CHANGE ME>
export REPORT_ERRORS=0
export ANKI_PROFILE=dev
export ANKI_BASE=/tmp/Anki2
export ANKIHUB_APP_URL=http://localhost:8000
export DISABLE_QT5_COMPAT=1
export QTWEBENGINE_CHROMIUM_FLAGS="--no-sandbox"
export DEVELOPMENT=True
export QTWEBENGINE_REMOTE_DEBUGGING=8080
export ANKIDEV=1
export LOGTERM=1
